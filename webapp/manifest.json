{
	"_version": "1.2.0",
	"sap.app": {
		"_version": "1.2.0",
		"id": "pm.tlsup",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/here/goes/your/serviceurl/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.44.6"
		}
	},
	"sap.ui": {
		"_version": "1.2.0",
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_bluecrystal"]
	},
	"sap.ui5": {
		"_version": "1.2.0",
		"rootView": {
			"viewName": "pm.tlsup.view.S0",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.36.0",
			"libs": {
				"sap.m": {
					"minVersion": "1.36.0"
				},
				"sap.ui.core": {
					"minVersion": "1.36.0"
				},
				"sap.ui.layout": {
					"minVersion": "1.36.0"
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pm.tlsup.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"metadataUrlParams": {
						"sap-documentation": "heading"
					},
					"defaultBindingMode": "TwoWay"
				}
			}
		},
		"services": {},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "pm.tlsup.view",
				"controlId": "root",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "list",
				"target": ["list"]
			}, {
				"pattern": "Notification/{id}",
				"name": "notification",
				"target": ["notification"]
			}, {
				"pattern": "Orders/{id}",
				"name": "order",
				"target": ["order"]
			}, {
				"pattern": "Notification/{id}/createOrder",
				"name": "order-create",
				"target": ["order-create"]
			}],
			"targets": {
				"list": {
					"viewName": "S1",
					"viewId": "S1",
					"viewLevel": 1
				},
				"notification": {
					"viewName": "S2",
					"viewId": "S2",
					"viewLevel": 2
				},
				"order": {
					"viewName": "S3",
					"viewId": "S3",
					"viewLevel": 2
				},
				"order-create": {
					"viewName": "S4",
					"viewId": "S4",
					"viewLevel": 3
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				}
			}
		}
	}
}