<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="{i18n>f1_title}">
		<content>
			<List mode="SingleSelectMaster" items="{/Maintainers}">
				<items>
					<StandardListItem title="{OpName}">
						<customData>
							<core:CustomData key="cidnum" value="{CidNum}" />
						</customData>
					</StandardListItem>
				</items>
			</List>
		</content>
		<buttons>
			<Button text="{i18n>f1_button_confirm}" press=".onConfirmMaintainer"/>
			<Button text="{i18n>f1_button_cancel}" press=".onCancelMaintainer"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>